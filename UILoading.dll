	UIGradientvc.Transparency = NumberSequence.new{NumberSequenceKeypoint.new(0.00, 0.00), NumberSequenceKeypoint.new(0.85, 0.00), NumberSequenceKeypoint.new(1.00, 1.00)}
	UIGradientvc.Parent = Load
	
	pcall(function()
		local function HidenToView()
			local k = game:GetService('TweenService')
			local enum = Enum.EasingStyle.Back

			HazardCenter1.Rotation = 0
			HazardCenter2.Rotation = 0
			HazardCenter3.Rotation = 0

			k:Create(Hazard,TweenInfo.new(0.6,enum),{Size = UDim2.new(0.300000012, 0, 0.300000012, 0)}):Play()
			k:Create(HazardCenter1,TweenInfo.new(0.5,enum),{Rotation = 23,Size = UDim2.new(0.5, 0, 0.5, 0),Position = UDim2.new(0.275000006, 0, 0.625, 0)}):Play()
			k:Create(HazardCenter2,TweenInfo.new(0.6,enum),{Rotation = 144.500,Size = UDim2.new(0.5, 0, 0.5, 0),Position = UDim2.new(0.49000001, 0, 0.252000004, 0)}):Play()
			k:Create(HazardCenter3,TweenInfo.new(0.7,enum),{Rotation = -100,Size = UDim2.new(0.5, 0, 0.5, 0),Position = UDim2.new(0.709999979, 0, 0.620000005, 0)}):Play()

			k:Create(Hazardtick1,TweenInfo.new(0.8,enum),{Rotation = 56.000,Size = UDim2.new(1, 0, 1, 0),Position = UDim2.new(0.700254142, 0, 0.413823307, 0)}):Play()
			k:Create(Hazardtick1_2,TweenInfo.new(0.9,enum),{Rotation = -56.000,Size = UDim2.new(1, 0, 1, 0),Position = UDim2.new(0.318839461, 0, 0.356033206, 0)}):Play()
			k:Create(Hazardtick3,TweenInfo.new(1,enum),{Rotation = -182.000,Size = UDim2.new(1, 0, 1, 0),Position = UDim2.new(0.463314712, 0, 0.720110834, 0)}):Play()

		end

		local function ViewToHiden()
			local k = game:GetService('TweenService')
			local enum,h = Enum.EasingStyle.Back,Enum.EasingDirection.In

			k:Create(Hazard,TweenInfo.new(1,enum,h),{Size = UDim2.new(0,0,0,0)}):Play()
			k:Create(HazardCenter1,TweenInfo.new(0.9,enum,h),{Rotation = 23,Size = UDim2.new(0,0,0,0),Position = UDim2.new(0.5,0,0.5,0)}):Play()
			k:Create(HazardCenter2,TweenInfo.new(0.8,enum,h),{Rotation = 144.500,Size = UDim2.new(0,0,0,0),Position = UDim2.new(0.5,0,0.5,0)}):Play()
			k:Create(HazardCenter3,TweenInfo.new(0.7,enum,h),{Rotation = -100,Size = UDim2.new(0,0,0,0),Position = UDim2.new(0.5,0,0.5,0)}):Play()

			k:Create(Hazardtick1,TweenInfo.new(0.6,enum,h),{Rotation = 56.000,Size = UDim2.new(0,0,0,0),Position = UDim2.new(0.5,0,0.5,0)}):Play()
			k:Create(Hazardtick1_2,TweenInfo.new(0.55,enum,h),{Rotation = -56.000,Size = UDim2.new(0,0,0,0),Position = UDim2.new(0.5,0,0.5,0)}):Play()
			k:Create(Hazardtick3,TweenInfo.new(5,enum,h),{Rotation = -182.000,Size = UDim2.new(0,0,0,0),Position = UDim2.new(0.5,0,0.5,0)}):Play()

		end

		HidenToView()

		task.wait(1.5)

		game:GetService('TweenService'):Create(Hazard,TweenInfo.new(1.5,Enum.EasingStyle.Back,Enum.EasingDirection.InOut),{Rotation = -240}):Play()

		task.wait(0.1)

		local UI = game:GetService('TweenService'):Create(Hazard,TweenInfo.new(1.5,Enum.EasingStyle.Back,Enum.EasingDirection.InOut),{Position = UDim2.new(0.300000012, 0, 0.5, 0)})

		UI:Play()

		task.wait(1.5)

		game:GetService('TweenService'):Create(TextLabel,TweenInfo.new(0.5),{Size = UDim2.new(0.497329533, 0, 0.101570904, 0)}):Play()
		game:GetService('TweenService'):Create(UIGradient,TweenInfo.new(1.45,Enum.EasingStyle.Quint),{Offset = Vector2.new(0.5,0)}):Play()

		task.wait(3)

		Load.Visible = true
		repeat task.wait() until game:IsLoaded()

		Load.Visible = false

		game:GetService('TweenService'):Create(UIGradient,TweenInfo.new(1.45,Enum.EasingStyle.Quint),{Offset = Vector2.new(-1,0)}):Play()

		game:GetService('TweenService'):Create(Hazard,TweenInfo.new(1.5,Enum.EasingStyle.Back,Enum.EasingDirection.InOut),{Rotation = 0}):Play()

		task.wait(0.1)

		game:GetService('TweenService'):Create(Hazard,TweenInfo.new(1.5,Enum.EasingStyle.Back,Enum.EasingDirection.InOut),{Position = UDim2.fromScale(0.5,0.5)}):Play()

		task.wait(0.3)

		ViewToHiden()

		task.wait(1.4)
	end)
	
	task.wait()
	
	Loader:Destroy()
end

return PEDO
