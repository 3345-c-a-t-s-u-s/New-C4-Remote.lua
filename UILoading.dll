	UIGradient.Parent = TextLabel
	UIGradient.Rotation = 15
	
	local Load = Instance.new("TextLabel")
	local UIGradientvc = Instance.new("UIGradient")
	
	Load.Name = "Load"
	Load.Parent = Loader
	Load.AnchorPoint = Vector2.new(0.5, 0.5)
	Load.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Load.BackgroundTransparency = 1.000
